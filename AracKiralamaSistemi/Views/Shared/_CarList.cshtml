@using AracKiralama.Entity
@model IEnumerable<AracKiralama.Entity.araclar>

<head>
    

</head>

@foreach (araclar item in Model)
    {
        <div class="prod_box">
            <div class="top_prod_box"></div>
            <div class="center_prod_box">
                <div class="product_title">
                    <a href="~/Home/Detail/@item.aracID">@item.aracMarka</a>
                </div>
                <div class="product_img">
                    <a href="~/Home/Detail/@item.aracID"><img src="~/Upload/@item.photo" alt="" border="0" width="100" height="100" /></a>
                </div>
                <div class="prod_price">
                    <span class="price">@Html.Label(string.Format("{0:#.00}₺", item.fiyat))</span>
                </div>
            </div>
            <div class="bottom_prod_box"></div>
            <div class="prod_details_tab">
                <a href="~/Home/Detail/@item.aracID" class="prod_details">Detay</a>
            </div>
        </div>}